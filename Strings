127.0.0.1:6379> set user:1 Bhuvan
OK
127.0.0.1:6379> set user:2 Bopanna
OK
127.0.0.1:6379> get user:1
"Bhuvan"
127.0.0.1:6379> get user:2
"Bopanna"
127.0.0.1:6379> set msg:1 Hey
OK
127.0.0.1:6379> set msg:2 Hello
OK
127.0.0.1:6379> get msg:1
"Hey"
127.0.0.1:6379> mset user:3 Sharath user:4 Vinitha
OK
127.0.0.1:6379> mget user:3 user:4
1) "Sharath"
2) "Vinitha"
127.0.0.1:6379> set count 0
OK
127.0.0.1:6379> get count
"0"
127.0.0.1:6379> incr count 
(integer) 1
127.0.0.1:6379> get count
"1"
127.0.0.1:6379> incrby count 10
(integer) 11
127.0.0.1:6379> get count
"11"
